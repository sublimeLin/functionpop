                    <div class="row">
                        <div class="col-6 text-right">
                            使用者帳戶：
                        </div>
                        <div class="col-6 text-left">
                            <input type="text" name="cAccount" id="" placeholder="請在此填入信箱">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-6 text-right">
                            密碼：
                        </div>
                        <div class="col-6 text-left">
                            <input type="password" name="cPassword" id="" placeholder="請在此填入密碼">
                        </div>
                    </div>
                    <input type="submit" name="" id="">
                </form>
            </div>
    </div>
