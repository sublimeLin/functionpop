    <style>
        /* All */
        /* * {
            border: 1px red solid;
        } */
        /* My hr tag */
        .my_hr {
            width: 130px;
            background-color: orange; 
            height: 3px;
        }
        .my_product {
            cursor: pointer;
            margin-bottom: 2rem;
        }
        .my_product img {
            transition: 0.3s all;
            width: 100%;
            opacity: 0;       
        }
        .my_product:hover img{
            opacity: 1;
        }
        .my_product .rear_img{
            opacity: 1;
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
        }
        .my_product .rear_img:hover{
            opacity: 0;
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
        }
        /* Banner */
        /* 25. Banner */
        #banner {
            background-image: url("/img/product/background.png");
            width: 100%;
            height: 45vh;
            background-size: cover;
            background-position-x:center;
            background-position-y:center;
            background-repeat: no-repeat;
            background-attachment: fixed;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: flex-start;
        }
        #banner h1{
            color: #fff;
        }
        #banner h4 {
            color: #fff;
        }
        #banner button{
            background-color: coral;
        }
        #banner button:hover {
            background-color: #1d1d1d;
            color: azure;
        }
        .my_button {
            font-size: 0.8rem;
            font-weight: 900;
            outline: none;
            border: none;
            background-color: #1d1d1d;
            color: aliceblue;
            padding: 10px 20px;
            text-transform: uppercase;
            cursor: pointer;
            transition: 0.5s ease;
            border-radius: 5px;
        }
        .my_button:hover {
            background-color: rgb(255, 247, 0);
        }
        .star-progress{
            margin: 0;
            padding: 0;
            height: 25px;
            background-image: url("/img/product/star2.jpg"); /* 星星的大小大約是15px*/
            background-repeat: repeat-x; /* 作爲背景的星星圖案水平重複*/
            width: 100px; /* 進度條的寬度爲5個星星的寬度*/
        }
        .pIconbtn {
            /* border-radius: 5px; */
            width: 80px;
            height: 45px;
            background: white;
            color: black;
            border: 0px ;
            width: 60px; 
            height: 45px;
        }
        .star-progress-bar{
            float: left;
            width: 0; 
            height: 100%;
            text-align: center;
            -webkit-box-shadow: inset 0 -1px 0 rgba(0, 0, 0, .85);
            box-shadow: inset 0 -1px 0 rgba(0, 0, 0, .85);
            -webkit-transition: width .6s ease;
            -o-transition: width .6s ease;
            transition: width .6s ease;
            background-image: url("/img/product/star1.jpg");  /* 被點亮的星星圖案*/
            background-repeat: repeat-x; /* 水平重複*/
        }
        .star5 {
        box-sizing: border-box;
        width: var(--size, 24px);
        height: var(--size, 24px);
        }
        .rating {
        width: 120px;
        height: 24px;
        position: relative;
        background-color: gray;
        }
        .rating progress.rating-bg {
        -webkit-appearance: none;
        -moz-appearence: none;
        appearance: none;
        border: none;
        display: inline-block;
        height: 24px;
        width: 100%;
        color: orange;
        }
        .rating progress.rating-bg::-webkit-progress-value {
        background-color: rgb(239, 218, 28);
        }
        .rating progress.rating-bg::-moz-progress-bar {
        background-color: orange;
        }
        .rating svg {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        }
        #productOuter {
            width: 100%;
            text-align: center;
        }
        .productInner {
            display: inline-block;
        }
        /* All */
        .all {
            float: left;
            background-color: #2196F3;
            color: #ffffff;
            width: 100px;
            line-height: 100px;
            text-align: center;
            margin: 2px;
            display: none;
        }
            .show {
            display: block;
        }   
        .listBtn {
            background-color: white;
            border: 0px solid rgb(206, 200, 200);
            border-radius: 5px;
            padding: 1px 1px;
            margin: 1px 1px;
            color: #1d1d1d;
            text-align:left;
        }
        .listBtn:hover {
            background-color: white;
            opacity: 1;
            ;
            
        }
        .alink {
            text-decoration: none;
            color: #1d1d1d;
            text-align:left;
        }
        .alink:link {
            /* text-decoration: none; */
            color: black;
        }
        /* Back to Top */
        .back-to-top {
            display: none;
            position: fixed;
            bottom: 300px;
            right: 60px;
            z-index: 999;
            border: 0px ;
            outline: none;
            /* background-color: white; */
            opacity: 0.7;
            color: #000000;
            cursor: pointer;
            /* padding: 10px 15px 15px 15px; */
            background-color: #ffffff00;
            /* border-radius: 10px; */
        }
        /* .back-to-top:hover {
            color: rgb(251, 198, 63);
            border: 2px solid white;
        } */
    </style>
